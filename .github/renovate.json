{
    "extends": [
        "config:base"
    ],
    "baseBranches": [
        "develop"
    ],
    "packageRules": [
        {
            "matchUpdateTypes": [
                "minor",
                "patch"
            ],
            "matchCurrentVersion": "!/^0/",
            "automerge": true
        }
    ],
    "automergeType": "branch",
    "dependencyDashboard": false
}
